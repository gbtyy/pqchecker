<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Configuration>
<Configuration  status="fatal">	
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{|yyyyMMdd|HH:mm:ss|}%5p| %m%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="fatal">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
